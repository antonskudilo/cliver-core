{
  "name": "antonskudilo/cliver-core",
  "description": "Cliver core",
  "type": "library",
  "license": "MIT",
  "keywords": ["cli", "framework", "console", "commands", "php", "cliver"],
  "homepage": "https://github.com/antonskudilo/cliver-core",
  "require": {
    "php": ">=8.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.3"
  },
  "autoload": {
    "psr-4": {
      "Cliver\\Core\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Cliver\\Core\\Tests\\": "tests/"
    }
  },
  "bin": [
    "cliver"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
